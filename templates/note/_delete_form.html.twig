<form method="post" action="{{ path('note.delete', {'id': note.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ note.id) }}">
    <div class="button">
            <button class="btn fc-button-primary">Supprimer</button>
    </div>
</form>
